{
  "parserOptions": {
    "ecmaVersion": 5
  },
  "extends": "eslint:recommended",
  "rules": {
    "no-undef": "error",
    "no-unused-vars": ["warn", { "vars": "local" }]
  },
  "globals": {
    "caml_js_from_array": "readonly",
    "caml_js_from_string": "readonly",
    "caml_js_to_array": "readonly",
    "caml_js_wrap_callback": "readonly",
    "joo_global_object": "readonly",
    "updateJsPropFromOpt": "readonly",
    "jsNullFromOption": "readonly",
    "jsNullToOption": "readonly",
    "jsUndefinedFromOption": "readonly",
    "setIfSomeMap": "readonly",
    "setIfSome": "readonly",
    "React": "readonly"
  },
  "env": {
    "browser": true
  }
}
