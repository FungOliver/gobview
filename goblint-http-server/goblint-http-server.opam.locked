opam-version: "2.0"
name: "goblint-http-server"
version: "0.1.0"
synopsis: "Goblint HTTP server"
maintainer: "Kerem Çakırer <kerem.cakirer@tum.de>"
authors: "Kerem Çakırer <kerem.cakirer@tum.de>"
license: "ISC"
homepage: "https://github.com/goblint/goblint-http-server"
bug-reports: "https://github.com/goblint/goblint-http-server/issues"
depends: [
  "angstrom" {= "0.15.0"}
